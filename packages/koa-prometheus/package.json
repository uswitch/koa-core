{
  "name": "@uswitch/koa-prometheus",
  "version": "1.1.8",
  "description": "üå°Ô∏è A configurable Prometheus data collector with Koa middleware",
  "main": "build/koa-prometheus.js",
  "scripts": {
    "test": "npm run test:default",
    "test:default": "ajv validate -s src/koa-prometheus.schema.json -d src/koa-prometheus.defaults.json",
    "lint": "eslint .",
    "------- BUILD": "-----------------------------------------------------",
    "prebuild": "del build",
    "build:src": "babel src --out-dir build --ignore test.js",
    "build:config": "cp src/koa-prometheus.*.json build/.",
    "build": "npm run build:src; npm run build:config",
    "build:watch": "npm-watch build",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uswitch/koa-core.git",
    "directory": "packages/koa-prometheus"
  },
  "watch": {
    "build": "src/*"
  },
  "author": "Dom Charlesworth <dgc336@gmail.com> (http://domcharlesworth.co.uk/)",
  "license": "MIT",
  "dependencies": {
    "metrics": "^0.1.21",
    "prom-client": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.2",
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-object-rest-spread": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@eslint/js": "^9.39.1",
    "ajv-cli": "^5.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "npm-run-all": "^4.1.3",
    "npm-watch": "^0.13.0"
  },
  "overrides": {
    "fast-json-patch": "^3.1.1"
  },
  "gitHead": "a56e66478b59fb05a6bd867993ba540b4b1c1699"
}
