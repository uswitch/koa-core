{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "test",
          "build:deps",
          "update:deps",
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "build:deps": {
      "dependsOn": [
        "^build:deps"
      ],
      "outputs": [
        "{projectRoot}/"
      ]
    },
    "update:deps": {
      "dependsOn": [
        "^update:deps"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/packages"
      ]
    }
  }
}
